{
  "version": "0.0.1",
  "manifest_version": 2,
  "name": "quickproxyauth",
  "description": "Quick proxy authorization add-on for Firefox",
  "browser_specific_settings": {
    "gecko": {
      "id": "{39204309420}",
      "strict_min_version": "42.0"
    }
  },
  "permissions": [
    "<all_urls>"
  ],
  "background": {
    "scripts": ["auth.js"]
  }
}
